env:
  browser: true
  node: true
extends: "eslint:recommended"
rules:
  space-before-function-paren:
    - error
    - always
  no-cond-assign: "off"
  no-undef: "off"
  indent:
    - error
    - 2
  linebreak-style:
    - error
    - unix
  quotes:
    - error
    - single
  semi:
    - error
    - always
