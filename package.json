{
  "name": "asciidoctor.js",
  "version": "1.5.5-3",
  "description": "A JavaScript AsciiDoc processor, cross-compiled from the Ruby-based AsciiDoc implementation, Asciidoctor, using Opal",
  "main": "dist/npm/asciidoctor.js",
  "engines": {
    "node": ">=0.12",
    "npm": ">=3.0.0"
  },
  "files": [
    "dist/npm/asciidoctor.js",
    "dist/npm/asciidoctor-docbook.js",
    "dist/css/asciidoctor.css",
    "LICENSE",
    "README.adoc"
  ],
  "scripts": {
    "test": "node npm/test/unsupported-features.js && node npm/test/jasmine-bower.js && node npm/test/jasmine-bower-min.js && node npm/test/jasmine-npm.js && node npm/test/jasmine-webpack.js && karma start",
    "build": "node npm/build.js && npm run test",
    "dist": "cross-env MINIFY=1 node npm/dist.js",
    "lint": "eslint src spec npm",
    "package": "cross-env MINIFY=1 node npm/build.js && cross-env MINIFY=1 npm run test",
    "examples": "node npm/examples.js",
    "server": "node npm/server.js",
    "benchmark": "node npm/benchmark.js",
    "release": "cross-env MINIFY=1 node npm/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asciidoctor/asciidoctor.js.git"
  },
  "keywords": [
    "asciidoc",
    "asciidoctor",
    "opal",
    "javascript",
    "library"
  ],
  "authors": [
    "Dan Allen (https://github.com/mojavelinux)",
    "Guillaume Grossetie (https://github.com/mogztter)",
    "Anthonny Qu√©rouil (https://github.com/anthonny)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asciidoctor/asciidoctor.js/issues"
  },
  "homepage": "https://github.com/asciidoctor/asciidoctor.js",
  "dependencies": {
    "opal-runtime": "0.10.2",
    "xmlhttprequest": "~1.7.0"
  },
  "devDependencies": {
    "asciidoctor-reveal.js": "1.0.1",
    "asciidoctor-template.js": "1.5.5-3",
