    "bestikk-uglify": "0.1.1",
    "bower": "^1.5.3",
    "browserify": "^13.0.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.11.0",
    "http-server": "^0.9.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.7",
    "watchify": "^3.7.0",
    "webpack": "^1.13.3"
  }
